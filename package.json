{
  "name": "@elasticswap/sdk",
  "version": "0.0.30",
  "description": "Official SDK for ElasticSwap",
  "main": "dist/index.js",
  "repository": "https://github.com/elasticswap/elasticswap-sdk",
  "author": "ElasticSwap",
  "license": "GPL-3.0",
  "private": false,
  "engines": {
    "node": "=16.14.2",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "analyzer": "yarn stats && yarn webpack-bundle-analyzer stats.json",
    "build": "npx webpack -c ./webpack.config.production.js",
    "coverage": "c8 yarn test",
    "dev": "yarn prettier:js && yarn lint:js && npx webpack -c ./webpack.config.js --watch",
    "docs": "npx jsdoc2md -c .jsdoc.json src/index.mjs > dist/readme.md",
    "lint": "yarn run prettier && yarn run lint:js && yarn run lint:test",
    "lint:js": "npx eslint src/*.mjs src/**/*.mjs --fix",
    "lint:test": "npx eslint test/*.mjs test/**/*.mjs --fix",
    "prerelease": "yarn lint && yarn build",
    "prettier": "yarn run prettier:js && yarn run prettier:test",
    "prettier:js": "npx prettier --write src/*.mjs --write src/**/*.mjs --write src/**/**/*.mjs --write src/**/**/**/*.mjs",
    "prettier:test": "npx prettier --write test/*.mjs --write test/**/*.mjs --write test/**/**/*.mjs --write test/**/**/**/*.mjs",
    "release": "npm publish --access=public",
    "stats": "npx webpack -c ./webpack.config.js --json > stats.json",
    "test": "yarn run prettier:test && yarn run lint:test && yarn run test:run",
    "test:run": "mocha -timeout 30000 -exit './test/**/**.test.mjs' --experimental-json-modules"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-import-assertions": "^7.16.7",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@elasticswap/elasticswap": "0.0.11",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "c8": "^7.11.0",
    "chai": "^4.3.4",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "esm": "^3.2.25",
    "hardhat": "^2.6.7",
    "hardhat-deploy": "^0.9.4",
    "jsdoc-to-markdown": "^7.1.1",
    "mocha": "^9.1.3",
    "node-fetch": "^3.0.0",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "bnc-notify": "^1.9.1",
    "ethers": "^5.5.1",
    "ethers-multicall": "^0.2.2",
    "is-plain-object": "^5.0.0",
    "rxjs": "^7.4.0"
  }
}
